 <%= form_for(model) do |f| %>
  <% if @model.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@model.errors.count, "error") %> prohibited this micropost from being saved:</h2>

      <ul>
      <% @model.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <%= f.hidden_field :employee_id, class: 'form-control',required:"required" %>
  <div class="form-group">
    <%= f.label :start_year, class: 'control-label' %><br>
    <%= f.text_field :start_year, class: 'form-control',required:"required" %>
    <span class="help-block"></span>
  </div>

   <div class="form-group">
     <%= f.label :end_year, class: 'control-label' %><br>
     <%= f.text_field :end_year, class: 'form-control',required:"required" %>
     <span class="help-block"></span>
   </div>

   <div class="form-group">
     <%= f.label :placeeducation, class: 'control-label' %><br>
     <%= f.text_field :placeeducation, class: 'form-control',required:"required" %>
     <span class="help-block"></span>
   </div>

  <div class="actions">
    <%= f.submit 'Save', class: 'btn btn-primary' %>
  </div>
<% end %>
